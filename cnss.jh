
entity Agent {
	id Integer,
    nom String,
	prenom String,
    email String,
    adresse String
    
}

entity Client {
	id Integer ,
	nom String,
	prenom String,
    naissance Integer,
	email String,
	telephone Integer,
	salaire Long,
    adresse String
}

entity Profession {
    id Integer ,
    idClient Integer,
	secteur String,
	poste String,
	dateDeb Integer,
    dateFin Integer,
    periode Integer
}

entity Pension {
	id Integer,
    montant Float
}

relationship OneToMany {
	Agent{client} to Client{agent(nom)}
}

relationship OneToOne {
	Client{pension(montant)} to Pension{client(nom)}
    Client{profession} to Profession{client}
}

relationship OneToOne {
    Pension{profession} to Profession{pension}
    Pension{client(nom)} to Client{pension(montant)}
}

 dto * with mapstruct
 service all with serviceImpl




